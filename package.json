{
  "name": "grpc-node",
  "version": "1.0.0",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.3.6",
    "@grpc/proto-loader": "^0.6.4",
    "google-protobuf": "^3.17.3",
    "grpc_tools_node_protoc_ts": "^5.3.1",
    "grpc-tools": "^1.11.2",
    "grpc-web": "^1.2.1",
    "webpack": "^5.48.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.3",
    "@types/long": "^4.0.1",
    "@types/node": "^16.4.12",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "typescript": "^4.3.5",
    "webpack-cli": "^4.7.2"
  },
  "scripts": {
    "build": "npx tsc",
    "bundle": "webpack -o ./web/dist/ ./web/client-web.js",
    "note": "node dist/note.js",
    "start:server": "node dist/server.js",
    "start:client": "node dist/client.js",
    "compile": "npx grpc_tools_node_protoc --proto_path=src/protofile src/protofile/*.proto --js_out=import_style=commonjs,binary:src/pb --grpc_out=grpc_js:src/pb --ts_out=grpc_js:src/pb",
    "compile:proto-grpc_web": "protoc --proto_path=src/protofile src/protofile/*.proto --js_out=import_style=commonjs,binary:src/pb --grpc-web_out=import_style=commonjs,mode=grpcwebtext:src/pb",
    "compile:proto": "protoc src/protofile/*.proto --js_out=import_style=commonjs,binary:.",
    "compile:grpc_web": "protoc src/protofile/*.proto --grpc-web_out=import_style=commonjs,mode=grpcwebtext:."
  }
}
